<p id="notice"><%= notice %></p>

<%= render @wiki_post %>

<div>
  <h1><%= @wiki_post.title %></h1>
  
  <%= image_tag @wiki_post.image%>

  <p><%= @wiki_post.description %></p>

  <div><%= render "meta", wiki_post: @wiki_post %></div>

  <%= link_to "Edit", edit_wiki_post_path(@wiki_post) %> |
  <%= link_to "Back", wiki_posts_path %>

  <%= button_to "Destroy", @wiki_post, method: :delete %>
</div>
